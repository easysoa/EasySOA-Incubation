<package type="addon" name="easy-soa" version="@VERSION@">
  <title>EasySOA - Service Registry</title>
  <description>
    <p>This addon adds the "sample project" functionality to Nuxeo.</p>
    <p>Put here an HTML description of your Marketplace package.</p>
  </description>
  <home-page>http://www.easysoa.org/</home-page>
  <vendor>EasySOA</vendor>
  <installer restart="true" />
